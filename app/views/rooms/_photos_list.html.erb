<% if @room.images.attached? %>
    <br>
    <div class="row">
      <%  @room.images.each do |image| %>
        <div class="col-md-4">
            <div class="panel panel-default">
                <div class="panel-heading preview">
                    <%= image_tag image %>
                </div>
            </div>
        </div>
      <% end %>
    </div>
<% end %>